    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "sass": "^1.69.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass": "sass src/Assets/styles/main.scss src/assets/styles/css/index.css",
    "watch:sass": "sass src/assets/styles/main.scss src/assets/styles/css/index.css --watch",
    "start:sass": "npm-run-all -p watch:sass start",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/assets/styles/css/*.css -d src/assets/styles/css",
    "dev": "npm-run-all -p watch:sass start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
